{
    "name": "devuniverse/laravel-blog",
    "description": "A blog package for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Arpad Olasz",
            "email": "ishimwe@gmail.com",
            "homepage": "https://wingsline.com"
        }
    ],
    "homepage": "https://github.com/wingsline/blog",
    "keywords": ["Laravel", "Blog"],
    "require": {
        "php": "^7.4",
        "illuminate/support": "~7",
        "spatie/laravel-backup": "^6.0",
        "spatie/laravel-csp": "^2.6",
        "spatie/laravel-feed": "^2.7",
        "spatie/laravel-flash": "^1.6.0",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-menu": "^3.4",
        "spatie/laravel-missing-page-redirector": "^2.6",
        "spatie/laravel-responsecache": "^6.5",
        "spatie/laravel-sluggable": "^2.3",
        "spatie/laravel-tags": "^2.7",
        "michelf/php-markdown": "^1.9",
        "intervention/image": "^2.5",
        "laravel/ui": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0",
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Wingsline\\Blog\\": "src/",
            "Wingsline\\Blog\\Database\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wingsline\\Blog\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wingsline\\Blog\\BlogServiceProvider"
            ]
        }
    }
}
